	<!-- Specifies whether the capabilities in the tasks should be repeated if there is more then one task. -->
	<!-- Default: false -->
	<repeat_caps>false</repeat_caps>
	
	<!-- Specifies whether workpieces should be simulated or not. -->
	<simulate_workpieces>false</simulate_workpieces>
	
	<!-- Specifies the amount of different capabilities in each robot. -->
	<!-- For each robot a random value between min and max is used. -->
	<!-- min must be greater or equals than 1 and max must be greater or equals than min -->
	<!-- Default: min=1, max=1 -->
	<redundancy min="16" max="16" />
	
	<!-- Specifies the amount of work pieces to be processed by the system in each run. -->
	<!-- Set to 0 for unlimited amount. -->
	<!-- Default: 0 -->
	<workpieces>1</workpieces>
	
	<!-- Specifies the processing time in each agent (robot and transporter). -->
	<!-- For each agent a random value between min and max is used. -->
	<!-- max must be greater or equals than min. If not, the default is assumed-->
	<!-- If the processing time is set to 0 the simulation will be optimized for a event driven simulation -->
	<!-- using the Event Clock in Jadex. A processing time greater 0 results in a simulation optimized for -->
	<!-- the System Clock in Jadex. -->
	<!-- Default: min=0, max=0 -->
	<processing_time min="5" max="10" />
	
	<!-- Specifies the buffer size for each robot. -->
	<!-- For each robot a random value between min and max is used. -->
	<!-- max must be greater or equals than min. If not, the default is assumed-->
	<!-- Default: min=5, max=5 -->
	<buffer_size min="8" max="8" />
	
	<!-- Specifies how often this configuration is automatically executed by the system. -->
	<!-- Default: 1 -->
	<runs>2</runs>
	
	<!-- Specifies the logging interval for the timelord agent. Specify a value of 0 for a event driven simulation. -->
	<!-- Default: 0 -->
	<timelord_interval>1</timelord_interval>
	
	<!-- Specifies the time messages are delayed during the reconfiguration process. A value of 0 should be used for -->
	<!-- an event driven simulation. Else a value greater then 0 should be specified. -->
	<reconf_time>1</reconf_time>
</simulation_configuration>