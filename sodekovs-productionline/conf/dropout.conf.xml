<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<dropout_conf>
	<!-- rate: this config is repeated every x seconds  -->
	<!-- count: this config is repeated x times; set to 0 for infinite loop  -->
	<configuration rate="1" count="1">
		<query>
			<category>NONE</category>
			<agents>
				<agent>Robot3</agent>
			</agents>
		</query>
		<action type="DROP">
			<mode>STATIC</mode>
			<capabilities>
				<capability>C</capability>
			</capabilities>
		</action>
	</configuration>
</dropout_conf>