<?xml version="1.0" encoding="UTF-8"?>
<!--
  Scenario.
-->
<componenttype xmlns="http://jadex.sourceforge.net/jadex"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jadex.sourceforge.net/jadex
						http://jadex.sourceforge.net/jadex-component-2.0.xsd"
	name="Scenario" package="jadex.micro.examples.ws">
	
	<imports>
		<import>jadex.bridge.service*</import>
	</imports>
	
	<componenttypes>
		<componenttype name="User" filename="jadex/micro/examples/ws/UserAgent.class"/>
		<componenttype name="WebService" filename="jadex/micro/examples/ws/WebServiceAgent.class"/>
	</componenttypes>
	
	<configurations>
		<configuration name="default">
			<components>
				<component name="user" type="User"/>
				<component name="service" type="WebService"/>
			</components>
		</configuration>
	</configurations>
	
</componenttype>