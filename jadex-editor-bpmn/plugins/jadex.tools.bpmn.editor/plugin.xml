<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
	<extension
         point="org.eclipse.ui.editors">
      <editor
            class="jadex.editor.bpmn.editor.JadexBpmnEditor"
            matchingStrategy="org.eclipse.stp.bpmn.diagram.part.BpmnMatchingStrategy"
            contributorClass="jadex.editor.bpmn.editor.JadexBpmnEditorContributor"
            extensions="bpmn_diagram"
            default="true"
            icon="icons/bpmn.png"
            id="jadex.editor.bpmn.JadexBpmnEditor"
            name="Jadex BPMN Editor">
      </editor>
   </extension>
   
   <!-- custom edit part provider -->
   <extension point="org.eclipse.gmf.runtime.diagram.ui.editpartProviders">
      <editpartProvider class="jadex.editor.bpmn.diagram.providers.JadexEditPartProvider">
         <Priority name="Highest"/>
      </editpartProvider>
   </extension>
   
   <!-- Jadex BPMN - Preference page -->
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="jadex.editor.bpmn.editor.preferences.JadexPreferencesPage"
            id="jadex.editor.bpmn.editor.preferences.JadexPreferencesPage"
            name="Jadex">
      </page>
      <page
            category="jadex.editor.bpmn.editor.preferences.JadexPreferencesPage"
            class="jadex.editor.bpmn.editor.preferences.JadexBpmnEditorPreferencePage"
            id="jadex.editor.bpmn.editor.preferences.JadexBpmnEditorPreferencePage"
            name="BPMN Editor">
      </page>
      <page
      		category="jadex.editor.bpmn.editor.preferences.JadexBpmnEditorPreferencePage"
            class="jadex.editor.bpmn.editor.preferences.JadexBpmnClassLoaderPreferencePage"
            id="jadex.editor.bpmn.editor.preferences.JadexBpmnClassLoaderPreferencePage"
            name="Task Classloader">
      </page>
   </extension>
   
   <!-- Jadex BPMN - Preference initializer -->
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="jadex.editor.bpmn.editor.preferences.JadexBpmnPluginPreferenceInitializer">
      </initializer>
   </extension>
   
   
   <!-- add a jadex property tab to the editor-->
