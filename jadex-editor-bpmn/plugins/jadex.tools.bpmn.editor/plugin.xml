           typeMapper="jadex.editor.bpmn.editor.properties.template.EModelTypeMapper">
        <propertyCategory category="Jadex"/>
     </propertyContributor>
  	</extension>
  
  	<extension 
  		point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
     <propertyTabs 
