<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<artifactId>jadex-distribution-pro</artifactId>
	<packaging>jar</packaging>
	<name>jadex-distribution-pro</name>
	<parent>
		<groupId>net.sourceforge.jadex</groupId>
		<artifactId>jadex-parent-pro</artifactId>
		<version> 2.4-SNAPSHOT </version>
		<relativePath>../jadex-parent-pro/pom.xml</relativePath>
	</parent>

	<description>
		The pro distribution of the Jadex middleware includs
		all modules of the standard distribution plus several
		extra modules not available as open source.
	</description>

	<dependencies>
		<dependency>
			<groupId>net.sourceforge.jadex</groupId>
			<artifactId>jadex-distribution-standard</artifactId>
			<version>${project.version}</version>
		</dependency>
 		<dependency>
			<groupId>net.sourceforge.jadex</groupId>
			<artifactId>jadex-platform-extension-maven</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>net.sourceforge.jadex</groupId>
			<artifactId>jadex-platform-extension-securetransport</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>net.sourceforge.jadex</groupId>
			<artifactId>jadex-platform-extension-relay-standalone</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>net.sourceforge.jadex</groupId>
			<artifactId>jadex-platform-extension-management</artifactId>
			<version>${project.version}</version>
		</dependency>
	</dependencies>
</project>
