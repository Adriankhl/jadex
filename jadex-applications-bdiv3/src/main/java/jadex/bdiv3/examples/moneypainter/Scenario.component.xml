<componenttype xmlns="http://jadex.sourceforge.net/jadex"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jadex.sourceforge.net/jadex
						http://jadex.sourceforge.net/jadex-component-2.5.xsd"
	name="Scenario" package="jadex.bdiv3.examples.moneypainter">
	
	<componenttypes>
		<componenttype name="rich" filename="jadex/bdiv3/examples/moneypainter/RichBDI.class"/>
		<componenttype name="painter" filename="jadex/bdiv3/examples/moneypainter/PainterBDI.class"/>
	</componenttypes>
	
	<configurations>
		<configuration name="One Rich, one Painter">
			<components>
				<component type="rich"/>
				<component type="painter"/>
			</components>
		</configuration>
		
		<configuration name="One Rich, three Painters">
			<components>
				<component type="rich"/>
				<component type="painter" number="3"/>
			</components>
		</configuration>
	</configurations>
</componenttype>