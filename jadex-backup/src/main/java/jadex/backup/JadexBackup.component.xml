<?xml version="1.0" encoding="UTF-8"?>
<!--
  Master component of Jadex Backup instance.
-->
<componenttype xmlns="http://jadex.sourceforge.net/jadex"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jadex.sourceforge.net/jadex
						http://jadex.sourceforge.net/jadex-component-2.3.xsd"
	name="JadexBackup" package="jadex.backup">
	
    <componenttypes>
        <componenttype name="JobManager" filename="jadex/backup/job/JobManagerAgent.class"/>
        <componenttype name="ResourceProvider" filename="jadex/backup/resource/ResourceProviderAgent.class"/>
    </componenttypes>
    
    <configurations>
        <configuration name="test">
            <components>
                <component type="ResourceProvider">
                    <arguments>
                        <argument name="dir">
                            "test"
                        </argument>
                    </arguments>
                </component>
                <component type="ResourceProvider">
                    <arguments>
                        <argument name="dir">
                            "target"
                        </argument>
                    </arguments>
                </component>
                <component type="JobManager"/>
        	</components>
        </configuration>
    </configurations>
</componenttype>