<?xml version="1.0" encoding="UTF-8"?>
<!--
  GPMN kernel component.
-->
<componenttype xmlns="http://jadex.sourceforge.net/jadex-component"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jadex.sourceforge.net/jadex-component
						http://jadex.sourceforge.net/jadex-component-2.0.xsd"
	name="kernel_gpmn" package="jadex.gpmn">
	
	<imports>
		<import>jadex.commons.*</import>
	</imports>
	
	<properties>
		<property name="kernel_type_extensions" class="String[]">
			new String[] {"gpmn"}
		</property>
	</properties>
	
	<services>
		<service name="gpmn_factory" class="jadex.bridge.IComponentFactory">
			new GpmnFactory($component.getServiceProvider(), new java.util.HashMap())
		</service>
	</services>
	
</componenttype>