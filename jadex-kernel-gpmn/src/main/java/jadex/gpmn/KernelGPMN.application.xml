<?xml version="1.0" encoding="UTF-8"?>
<!--
  GPMN kernel application.
-->
<applicationtype xmlns="http://jadex.sourceforge.net/jadex-application"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jadex.sourceforge.net/jadex-application
						http://jadex.sourceforge.net/jadex-application-2.0.xsd"
	name="kernel_gpmn" package="jadex.gpmn">
	
	<imports>
		<import>jadex.commons.*</import>
	</imports>
	
	<services>
		<service name="gpmn_factory" class="jadex.bridge.IComponentFactory">
			new GpmnFactory($component.getServiceProvider(), new java.util.HashMap())
		</service>
	</services>
</applicationtype>