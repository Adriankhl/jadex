<!--
	<H3>The chart provider</H3>
-->
<componenttype xmlns="http://jadex.sourceforge.net/jadex"
	xmlns:env="http://jadex.sourceforge.net/jadex-envspace"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jadex.sourceforge.net/jadex
	                    http://jadex.sourceforge.net/jadex-component-2.4.xsd
	                    http://jadex.sourceforge.net/jadex-envspace 
	                    http://jadex.sourceforge.net/jadex-envspace-2.4.xsd"
	name="ChartProvider" package="jadex.android.applications.demos.rest">
	
	<imports>
		<import>jadex.extension.rs.invoke.*</import>
		<import>jadex.android.applications.demos.rest.*</import>
	</imports>
	
	<services>
		<providedservice class="IChartService">
			<implementation>SRest.createServiceImplementation($component, 
				IChartService.class, IRSChartService.class)</implementation>
		</providedservice>
	</services>
	
</componenttype>
