	<!-- add custom commands to menu -->
	<extension point="org.eclipse.ui.popupMenus">
	<?gmfgen generated="false"?>
		<!-- BPMN Menu -->
	  <objectContribution
	    adaptable="false"
	    id="jadex.tools.gpmn.diagram.ui.objectContribution.PlanEditPart1"
	    objectClass="jadex.tools.gpmn.diagram.edit.parts.PlanEditPart">
	 <menu 
	    id="BPMN" 
	    label="BPMN" 
	    path="additions"> 
	    <separator name="group1"/>
	 </menu>
	 <action
	       class="jadex.tools.gpmn.diagram.part.SelectBPMNPlanFileAction"
	       definitionId="jadex.tools.gpmn.commands.bpmn.selectPlanFile"
	       enablesFor="1"
	       id="jadex.tools.gpmn.diagram.popup.SelectBPMNPlanFileActionID"
	       label="Select plan file"
	       menubarPath="BPMN/group1">
	 </action>
	  <action
	       class="jadex.tools.gpmn.diagram.part.OpenBPMNPlanFileAction"
	       definitionId="jadex.tools.gpmn.commands.bpmn.openPlanFile"
	       enablesFor="1"
	       id="jadex.tools.gpmn.diagram.popup.OpenBPMNPlanFileActionID"
	       label="Open plan file"
	       menubarPath="BPMN/group1">
	 </action>
	 </objectContribution>
	 <!-- GPMN Menu -->
	 <objectContribution
	    adaptable="false"
	    id="jadex.tools.gpmn.diagram.ui.objectContribution.SubProcessGoalEditPart1"
	    objectClass="jadex.tools.gpmn.diagram.edit.parts.SubProcessGoalEditPart">
	 <menu 
	    id="GPMN" 
	    label="GPMN" 
	    path="additions"> 
	    <separator name="group1"/>
	 </menu>
	 <action
	       class="jadex.tools.gpmn.diagram.part.SelectGPMNDiagramFileAction"
	       definitionId="jadex.tools.gpmn.commands.gpmn.selectDiagramFile"
	       enablesFor="1"
	       id="jadex.tools.gpmn.diagram.popup.SelectGPMNDiagramFileActionID"
	       label="Select diagram file"
	       menubarPath="GPMN/group1">
	 </action>
	  <action
	       class="jadex.tools.gpmn.diagram.part.OpenGPMNDiagramFileAction"
	       definitionId="jadex.tools.gpmn.commands.gpmn.openDiagramFile"
	       enablesFor="1"
	       id="jadex.tools.gpmn.diagram.popup.OpenGPMNDiagramFileActionID"
	       label="Open diagram file"
	       menubarPath="GPMN/group1">
	 </action>
	 </objectContribution>
	</extension>
	
