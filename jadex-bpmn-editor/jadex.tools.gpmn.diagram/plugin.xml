   <extension point="org.eclipse.ui.navigator.navigatorContent" id="navigator-content">
      <?gmfgen generated="true"?>
      <navigatorContent
            id="jadex.tools.gpmn.diagram.resourceContent" 
            name="%navigatorContentName" 
            priority="normal" 
            contentProvider="jadex.tools.gpmn.diagram.navigator.GpmnNavigatorContentProvider" 
            labelProvider="jadex.tools.gpmn.diagram.navigator.GpmnNavigatorLabelProvider"
            icon="icons/obj16/GpmnDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="gpmn_diagram"/>
               </and>
               <instanceof value="jadex.tools.gpmn.diagram.navigator.GpmnAbstractNavigatorItem"/>
           	   <adapt type="org.eclipse.gmf.runtime.notation.View">
           	      <test property="jadex.tools.gpmn.diagram.isShortcut"/>
           	   </adapt>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
         	   <instanceof value="jadex.tools.gpmn.diagram.navigator.GpmnAbstractNavigatorItem"/>
           	   <adapt type="org.eclipse.gmf.runtime.notation.View">
           	      <test property="jadex.tools.gpmn.diagram.isShortcut"/>
           	   </adapt>
            </or>
         </possibleChildren>
         <commonSorter 
               id="jadex.tools.gpmn.diagram.navigatorSorter" 
               class="jadex.tools.gpmn.diagram.navigator.GpmnNavigatorSorter">
            <parentExpression>
               <or>
	              <and>
    	             <instanceof value="org.eclipse.core.resources.IFile"/>
        	         <test property="org.eclipse.core.resources.extension" value="gpmn_diagram"/>
                  </and>
                  <instanceof value="jadex.tools.gpmn.diagram.navigator.GpmnAbstractNavigatorItem"/>
               </or>
            </parentExpression>
         </commonSorter>
      </navigatorContent>
