<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="jadex.tools.gpmn.presentation.GpmnModelWizardID"
            name="%_UI_GpmnModelWizard_label"
            class="jadex.tools.gpmn.presentation.GpmnModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/GpmnModelFile.gif">
         <description>%_UI_GpmnModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="jadex.tools.gpmn.presentation.GpmnEditorID"
            name="%_UI_GpmnEditor_label"
            icon="icons/full/obj16/GpmnModelFile.gif"
            extensions="gpmn"
            class="jadex.tools.gpmn.presentation.GpmnEditor"
            contributorClass="jadex.tools.gpmn.presentation.GpmnActionBarContributor">
      </editor>
   </extension>

</plugin>
