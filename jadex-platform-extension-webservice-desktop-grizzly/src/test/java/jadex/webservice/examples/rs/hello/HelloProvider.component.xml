<!--
	<H3>The hello provider</H3>
-->
<componenttype xmlns="http://jadex.sourceforge.net/jadex"
	xmlns:env="http://jadex.sourceforge.net/jadex-envspace"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jadex.sourceforge.net/jadex
	                    http://jadex.sourceforge.net/jadex-component-2.5.xsd
	                    http://jadex.sourceforge.net/jadex-envspace 
	                    http://jadex.sourceforge.net/jadex-envspace-2.5.xsd"
	name="HelloProvider" package="jadex.webservice.examples.rs.hello">
	
	<imports>
		<import>jadex.extension.rs.invoke.*</import>
	</imports>
	
	<services>
		<providedservice class="IHelloService">
			<implementation>SRest.createServiceImplementation($component, 
				IHelloService.class, IRSHelloService.class)</implementation>
		</providedservice>
	</services>
	
</componenttype>
