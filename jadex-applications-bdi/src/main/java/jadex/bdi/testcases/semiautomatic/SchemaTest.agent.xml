<?xml version="1.0" encoding="UTF-8"?>
<!--
	<H3>Test all Tags and Attributes of the XML Schema.</H3>

	Does nothing useful, but can be used e.g.
	to test the reader for completeness. 
-->
<agent xmlns="http://jadex.sourceforge.net/jadex"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jadex.sourceforge.net/jadex
	                    http://jadex.sourceforge.net/jadex-bdi-2.0.xsd"
	name="SchemaTest"	package="jadex.bdi.testcases.semiautomatic"
	master="false"	autoshutdown="false"	daemon="false"	suspend="false">
	
	<imports>
		<import>jadex.bdi.planlib.test.*</import>
	</imports>
	
	<capabilities >
<!--		<capability name="cx" file="Test.capability.xml"/> -->
	</capabilities>
	
	<beliefs>
		<belief name="bx" class="String" argument="true" evaluationmode="pull" result="true" updaterate="1000" exported="false" transient="false">
			<fact class="String" language="java" variable="x">
				"hello"
			</fact>
		</belief>
		<beliefref name="brx" argument="true" class="String" exported="false" result="true">
			<abstract/>
		</beliefref>
		<beliefset name="bsx" class="String">
			<facts>new String[]{"hello", "world"}</facts>
		</beliefset>
		<beliefsetref name="bsrx">
			<assignto ref=""/>
			<concrete ref=""/>
		</beliefsetref>
	</beliefs>
	
	<goals>
		<achievegoal name="">
			<parameter name="" class=""></parameter>
			<parameterset name="" class=""></parameterset>			
			<unique></unique>
			<creationcondition></creationcondition>
			<contextcondition></contextcondition>
			<dropcondition></dropcondition>
			<recurcondition></recurcondition>
			<deliberation></deliberation>
			<targetcondition></targetcondition>
			<failurecondition></failurecondition>
		</achievegoal>
		<achievegoalref name="">
			<abstract/>
		</achievegoalref>
		<maintaingoal name="">
			<maintaincondition></maintaincondition>
		</maintaingoal>
		<maintaingoalref name="">
			<abstract/>
		</maintaingoalref>
		<metagoal name="">
			<trigger></trigger>
		</metagoal>
		<metagoalref name="">
			<abstract/>
		</metagoalref>
		<performgoal name=""></performgoal>
		<performgoalref name="">
			<abstract/>
		</performgoalref>
		<querygoal name=""></querygoal>
		<querygoalref name="">
			<abstract/>
		</querygoalref>
	</goals>
	
	<plans></plans>
	
	<events></events>
	
	<expressions></expressions>

	<services>
		<requiredservice name="" class="">
			<binding/>
		</requiredservice>
	</services>
	
	<properties></properties>
	
	<componenttypes></componenttypes>
	
	<configurations></configurations>	
		
</agent>

