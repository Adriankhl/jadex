	xsi:schemaLocation="http://jadex.sourceforge.net/jadex
						http://jadex.sourceforge.net/jadex-component-2.0.xsd"
	name="EnvSupport" package="jadex.extension.envsupport">

	<imports>
		<import>jadex.bridge.*</import>
	</imports>
			
	<services>
		<providedservice name="envextension" class="IComponentFactoryExtensionService">
			<implementation class="EnvSupportExtensionService"/>
		</providedservice>
	</services>
	
</componenttype>