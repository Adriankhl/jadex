<!--
	<H3>The carriage environment interface standard example</H3>

	It can be used to launch the carriage environment interface standard example.
-->

<applicationtype xmlns="http://jadex.sourceforge.net/jadex-application"
	xmlns:eis="http://jadex.sourceforge.net/jadex-eisspace"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jadex.sourceforge.net/jadex-application 
	                    http://jadex.sourceforge.net/jadex-application-2.0.xsd
	                    http://jadex.sourceforge.net/jadex-eisspace 
	                    http://jadex.sourceforge.net/jadex-eisspace-1.0.xsd"
	name="Carriage" package="jadex.micro.examples.carriage">
	
	<imports>
		<import>jadex.adapter.base.envsupport.environment.*</import>
		<import>jadex.adapter.base.envsupport.math.*</import>
		<import>jadex.adapter.base.envsupport.dataview.*</import>
		<import>jadex.adapter.base.envsupport.observer.perspective.*</import>
		<import>jadex.bdi.planlib.*</import>
	</imports>
	
	<spacetypes>
		<eis:eisspacetype name="env" class="carriageexample.EnvironmentInterface">
		</eis:eisspacetype>
	</spacetypes>
	
	<agenttypes>
		<agenttype name="PushingAgent" filename="jadex/carriageexample/PushingAgent.class"/>
		<agenttype name="AlternatingAgent" filename="jadex/carriageexample/AlternatingAgent.class"/>
	</agenttypes>
	
	<applications>
		<application name="carriage">
			<spaces>
				<eis:eisspace name="myenv" type="env">
				</eis:eisspace>
			</spaces>

			<agents>
				<agent type="PushingAgent" />
				<agent type="AlternatingAgent" />
			</agents>
		</application>
	</applications>

</applicationtype>
