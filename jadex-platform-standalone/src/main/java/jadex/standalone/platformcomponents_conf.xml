<?xml version="1.0" encoding="UTF-8"?>
<!--
  Standalone platform properties.
-->
<properties xmlns="http://jadex.sourceforge.net/jadexconf"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jadex.sourceforge.net/jadexconf
	                    http://jadex.sourceforge.net/configuration.xsd"
	name="standalone_conf">
	
	<properties type="platform_standalone" name="lars">
  		<properties type="component" name="cms">
  			<property type="model">jadex/bdi/cmsagent/CMS.agent.xml</property>
  			<property type="daemon">true</property>
  			<property type="master">true</property>
  		</properties>
  		
		<properties type="component" name="df">
			<property type="model">jadex/bdi/dfagent/DF.agent.xml</property>
  			<property type="daemon">true</property>
  			<property type="master">true</property>
		</properties>
 		
 		<property type="component" name="jcc">jadex/tools/jcc/JCC.agent.xml</property>
	</properties>
</properties>